// Like, postgresql://user:password@localhost:5432/database/schema
datasource db {
  provider = "postgresql"
  url      = env("NETLIFY_BETA_PG_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}
